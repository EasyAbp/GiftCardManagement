@page
@using Microsoft.AspNetCore.Mvc.Localization
@using MyProject.Localization
@using MyProject.Web.Pages.UserGifts.UserGift
@using Volo.Abp.AspNetCore.Mvc.UI.Layout
@inject IHtmlLocalizer<MyProjectResource> L
@model IndexModel
@inject IPageLayout PageLayout
@{
    PageLayout.Content.Title = L["UserGift"].Value;
    PageLayout.Content.MenuItemName = "UserGift";
}

@section scripts
{
    <abp-script src="/Pages/UserGifts/UserGift/index.js" />
}
@section styles
{
    <abp-style src="/Pages/UserGifts/UserGift/index.css"/>
}
@section content_toolbar
{
    <abp-button id="NewUserGiftButton"
            text="@L["CreateUserGift"].Value"
            icon="plus"
            size="Small"
            button-type="Primary" />
}

<abp-card>
    <abp-card-body>
        <abp-table striped-rows="true" id="UserGiftTable" class="nowrap">
            <thead>
            <tr>
                <th>@L["Actions"]</th>
                <th>@L["UserGiftUserId"]</th>
                <th>@L["UserGiftGiftId"]</th>
            </tr>
            </thead>
        </abp-table>
    </abp-card-body>
</abp-card>