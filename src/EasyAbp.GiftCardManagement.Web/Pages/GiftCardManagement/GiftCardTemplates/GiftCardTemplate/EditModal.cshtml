@page
@inherits EasyAbp.GiftCardManagement.Web.Pages.GiftCardManagementPage
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model EasyAbp.GiftCardManagement.Web.Pages.GiftCardTemplates.GiftCardTemplate.EditModalModel
@{
    Layout = null;
}
<abp-dynamic-form abp-model="GiftCardTemplate" data-ajaxForm="true" asp-page="EditModal">
    <abp-modal>
        <abp-modal-header title="@L["EditGiftCardTemplate"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="Id" />
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>